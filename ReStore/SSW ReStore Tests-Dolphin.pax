!SSWReStoreTest class methodsFor!

incrementQueryCount

	queryCount isNil
		ifTrue: [queryCount := 1] 
		ifFalse: [queryCount := queryCount + 1]!

isDolphin

	^true!

listModelClass

	^ListModel!

listModelOn: aCollection

	^ListModel on: aCollection!

queryCount

	^queryCount!

resetQueryCount

	queryCount := 0! !
